<h1>User Profile</h1>

<%= link_to "Home", root_path %>
<%= link_to "Back", :back %>

<h2>Created Events</h2>
<div class="events-container">
  <% @user.created_events.each do |e| %>
    <div class="event">
      <h4><%= e.name %></h4>

      <%= link_to "See more", e %>
    </div>
  <% end %>
</div>
